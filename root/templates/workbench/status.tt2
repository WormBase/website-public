<a class="bench-update" 
    wbid="[% c.stash.star.wbid %]" 
    ref="[% c.uri_for('/rest/workbench') %]" 
    name="[% c.stash.star.name %]" 
    is_obj="[% c.stash.star.is_obj %]" 
    save_to="[% c.stash.star.save_to %]" 
    href="[% c.stash.star.url %]">
  <div id="save" class="ui-icon ui-icon-stars ui-icon-star-[% c.stash.star.value == 1 ? 'yellow' : 'gray'; %]"></div>
</a>   